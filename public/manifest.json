{
    "$schema": "http://json.schemastore.org/chrome-manifest",
    "name": "To Do - Link Saver",
    "version": "1.0",
    "description": "Chromium extension for saving links to your Microsoft To Do list.",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "identity",
        "identity.email"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        "index.html",
        "redirect.html"
    ],
    "incognito": "split"
  }
