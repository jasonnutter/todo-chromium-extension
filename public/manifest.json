{
    "$schema": "http://json.schemastore.org/chrome-manifest",
    "name": "To-Do (Unofficial)",
    "version": "1.0.0",
    "description": "Unofficial Chromium extension for saving links to your Microsoft To-Do lists.",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "identity",
        "identity.email"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        "index.html",
        "redirect.html"
    ],
    "icons": {
        "16": "icon.png",
        "128": "icon.png",
        "256": "icon.png",
        "48": "icon.png"
    },
    "incognito": "not_allowed"
  }
